{
  "name": "teach-backend",
  "version": "1.0.0",
  "description": "TEACH Platform Backend API",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon",
    "build": "npx tsc --skipLibCheck --outDir dist --rootDir src",
    "start": "node dist/index.js",
    "start:prod": "npm run generate && npm run build && npm run start",
    "start:railway": "prisma migrate deploy && node start.js",
    "lint": "eslint src/**/*.ts",
    "test": "jest",
    "migrate": "prisma migrate dev",
    "migrate:prod": "prisma migrate deploy",
    "generate": "prisma generate",
    "seed": "ts-node prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@types/node": "^20.10.5",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "winston": "^3.11.0",
    "redis": "^4.6.11",
    "bull": "^4.11.5",
    "axios": "^1.6.2",
    "@aws-sdk/client-s3": "^3.478.0",
    "@aws-sdk/s3-request-presigner": "^3.478.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/jest": "^29.5.11",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "nodemon": "^3.0.2",
    "prisma": "^5.7.0"
  }
}